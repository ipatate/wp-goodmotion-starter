<header class="gm-header" >
  {% block header %}
    <div class="wrapper">
      <h1 class="hdr-logo" role="banner">
        <a class="hdr-logo-link" href="{{site.url}}" rel="home">{{site.name}}</a>
      </h1>
      <nav id="nav-main" class="gm-nav-main" role="navigation" aria-label="{{ __('Primary nav', 'goodmotion-theme') }}">
        {% include "menu.twig" with {'items': menu.get_items, 'home_url': home_url} %}
      </nav>
    <div class="gm-nav-button">
      <button
        type="button"
        aria-label="Nav button"
        aria-expanded="false"
        class="gm-burger"
      >
        <span></span>
        <span></span>
        <span></span>
      </button>
    </div>
    </div>
  {% endblock %}
</header>
